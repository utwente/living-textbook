        "ryakad/pandoc-php": "1.0.0",
        "sensio/framework-extra-bundle": "6.1.5",
        "sentry/sentry-symfony": "4.1.4",
        "spatie/pdf-to-image": "2.1.0",
        "stof/doctrine-extensions-bundle": "1.6.0",
        "symfony/asset": "4.4.27",
        "symfony/cache": "4.4.27",
        "symfony/config": "4.4.27",
        "symfony/console": "4.4.29",
