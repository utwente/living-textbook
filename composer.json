    "require-dev": {
        "drenso/phan-extensions": "3.5.1",
        "friendsofphp/php-cs-fixer": "3.35.1",
        "phpunit/phpunit": "10.4.2",
        "rector/rector": "0.18.6",
        "symfony/error-handler": "5.4.29",
        "symfony/phpunit-bridge": "5.4.31",
        "symfony/stopwatch": "5.4.21",
        "symfony/web-profiler-bundle": "5.4.31"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "component-dir": "public/elfinder-assets",
        "cache-dir": "/var/cache/composer",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=assets/js/_fos_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        },
        "unused": [
            "league/html-to-markdown",
            "symfony/messenger",
            "symfony/proxy-manager-bridge",
            "twig/cssinliner-extra",
            "twig/extensions",
            "twig/inky-extra",
            "twig/intl-extra"
        ]
    }
}
