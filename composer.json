        "symfony/http-foundation": "4.4.29",
        "symfony/http-kernel": "4.4.29",
        "symfony/lock": "4.4.27",
        "symfony/mailer": "4.4.27",
        "symfony/messenger": "4.4.27",
        "symfony/mime": "4.4.27",
        "symfony/monolog-bundle": "3.7.0",
        "symfony/options-resolver": "4.4.27",
        "symfony/process": "4.4.27",
        "symfony/property-access": "4.4.27",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.27",
        "symfony/routing": "4.4.27",
        "symfony/security": "4.4.29",
        "symfony/security-bundle": "4.4.27",
        "symfony/serializer": "4.4.*",
        "symfony/string": "5.3.3",
        "symfony/translation": "4.4.27",
        "symfony/translation-contracts": "2.4.0",
        "symfony/twig-bridge": "4.4.27",
        "symfony/twig-bundle": "4.4.27",
        "symfony/validator": "4.4.29",
        "symfony/webpack-encore-bundle": "1.12.0",
        "symfony/yaml": "4.4.29",
        "twig/cssinliner-extra": "3.3.0",
        "twig/extra-bundle": "3.3.1",
        "twig/inky-extra": "3.3.0",
        "twig/intl-extra": "3.3.0",
        "twig/twig": "2.14.6"
    },
    "require-dev": {
        "drenso/phan-extensions": "3.5.1",
        "symfony/phpunit-bridge": "4.4.27",
        "symfony/profiler-pack": "1.0.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "component-dir": "public/elfinder-assets",
        "cache-dir": "/var/cache/composer",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install %PUBLIC_DIR%/ckeditor --exclude=samples --exclude=adapters --clear=drop -q": "symfony-cmd",
            "fos:js-routing:dump --target=assets/js/_fos_js_routes.js --callback=\"import Routing from 'fos-routing'; Routing.setData\"": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "unused": [
            "league/html-to-markdown",
            "symfony/messenger",
            "symfony/proxy-manager-bridge",
            "twig/cssinliner-extra",
            "twig/extensions",
            "twig/inky-extra",
            "twig/intl-extra"
        ]
    }
}
